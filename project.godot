; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/quality/use_debanding=false

[application]

config/name="Space Pool"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://assets/default_bus_layout.tres"

[autoload]

AudioManager="*res://scripts/audio_manager.gd"

[display]

window/stretch/mode="canvas_items"

[physics]

2d/default_linear_damp=1.0
2d/sleep_threshold_linear=6.0
2d/sleep_threshold_angular=1.0
2d/time_before_sleep=0.1
2d/solver/solver_iterations=32

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
